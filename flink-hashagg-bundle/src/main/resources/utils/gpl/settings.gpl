# SVG output
set terminal svg size 500, 400 fname 'arial' fsize 10

# PNG output
# set terminal pngcairo size 500,400 enhanced font 'arial,10'

# line styles
set style line 1 lc rgb "#6C9AA7"
set style line 2 lc rgb "#FDA0A2"
set style line 3 lc rgb "#FFD1A2"
set style line 4 lc rgb "#8AD386"
set style fill solid

# sizing
set size ratio 0.5
set boxwidth 0.5

# disable exponential notation for y axis
set format y "%.0f"
